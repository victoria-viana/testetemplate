<p id="notice"><%= notice %></p>

<h1>Tipo Entradas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_entradas.each do |tipo_entrada| %>
      <tr>
        <td><%= tipo_entrada.nome %></td>
        <td><%= link_to 'Show', tipo_entrada %></td>
        <td><%= link_to 'Edit', edit_tipo_entrada_path(tipo_entrada) %></td>
        <td><%= link_to 'Destroy', tipo_entrada, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Entrada', new_tipo_entrada_path %>
