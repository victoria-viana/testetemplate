<p id="notice"><%= notice %></p>

<h1>Colaboradors</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Rg</th>
      <th>Celular</th>
      <th>Email</th>
      <th>Whatsapp</th>
      <th>Interno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @colaboradors.each do |colaborador| %>
      <tr>
        <td><%= colaborador.nome %></td>
        <td><%= colaborador.cpf %></td>
        <td><%= colaborador.rg %></td>
        <td><%= colaborador.celular %></td>
        <td><%= colaborador.email %></td>
        <td><%= colaborador.whatsapp %></td>
        <td><%= colaborador.interno %></td>
        <td><%= link_to 'Show', colaborador %></td>
        <td><%= link_to 'Edit', edit_colaborador_path(colaborador) %></td>
        <td><%= link_to 'Destroy', colaborador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Colaborador', new_colaborador_path %>
